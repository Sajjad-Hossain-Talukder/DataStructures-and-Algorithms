#include<bits/stdc++.h>
#define ll long long
#define pb push_back
using namespace std;

int main(){

    string s , p , text1 , text2 ;
    text1.pb('0');
    text2.pb('0');
    cin >> s >> p ;
    text1.append(s.begin() , s.end());
    text2.append(p.begin() , p.end());

    int d1 = text1.size(), d2 = text2.size() ;

    int dp[ d1 ][ d2 ] ;

    memset( dp , 0 , sizeof(dp) ) ;
    

    for( int i = 1 ; i < d1 ; i++ ){

        for( int j = 1 ; j < d2 ; j++ ){

            if( text1[i] == text2[j] ) dp[i][j] = dp[i-1][j-1] + 1 ;
            else dp[i][j] = max ( dp[i-1][j] , dp[i][j-1] ) ;

        }
    }

    /*
    for(int i = 0 ; i < d1 ; i++ ){
        for(int j = 0 ; j < d2 ; j++ ){
            cout << dp[i][j] << " ";
        }
        cout << endl;
    }

    */
    
    cout << "Longest Common Subsequence Length :  " << dp[d1-1] [d2-1]  << endl;





return 0;
}
