#include<bits/stdc++.h>
#define ll long long
#define pb push_back
using namespace std;

vector<bool>visited(1000,0);
vector<int>graph[1000];

void dfs ( int n ){
    cout<<" dfs "<<n<<" "<<endl;;
    if(visited[n]) return ;
    visited[n] = true;
    for(auto a : graph[n]){
        dfs(a);
    }
}

int main(){
    int node,edge,x,y;
    cin>>node>>edge;

    for( int i=1 ; i<=edge ; i+=1 ){
        cin>>x>>y;
        graph[x].pb(y);
        graph[y].pb(x);
    }
    
    for(int i = 0 ; i<node ; i+=1) 
        dfs(i);

    for(int i = 0 ; i<=6 ; i++ ){
            cout<<visited[i]<<" ";
    }
    cout<<endl;
return 0;
}
